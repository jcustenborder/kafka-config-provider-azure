{
  "name": "Staging",
  "prefix": "keyVault",
  "description": "The following example us prefixes all secrets with `staging-`.",
  "config": {
    "vault.url": "https://example.vault.azure.net/",
    "secret.prefix": "staging-"
  },
  "connectorConfig": {
    "username": "${keyVault:mysql-connector:username}",
    "password": "${keyVault:mysql-connector:password}"
  }
}